<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>GetPrice</servlet-name>
        <servlet-class>com.stockmarket.analysis.GetPrice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserRegistration</servlet-name>
        <servlet-class>com.stockmarket.loginandregistration.UserRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserLogin</servlet-name>
        <servlet-class>com.stockmarket.loginandregistration.UserLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>com.stockmarket.loginandregistration.AdminLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>com.stockmarket.logout.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuyShares</servlet-name>
        <servlet-class>com.stockmarket.analysis.BuyShares</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Analysis</servlet-name>
        <servlet-class>com.stockmarket.analysis.Analysis</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MapReduce</servlet-name>
        <servlet-class>com.stockmarket.mapreduce.MapReduce</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetPrice</servlet-name>
        <url-pattern>/GetPrice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserRegistration</servlet-name>
        <url-pattern>/UserRegistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserLogin</servlet-name>
        <url-pattern>/UserLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuyShares</servlet-name>
        <url-pattern>/BuyShares</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Analysis</servlet-name>
        <url-pattern>/Analysis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MapReduce</servlet-name>
        <url-pattern>/MapReduce</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
